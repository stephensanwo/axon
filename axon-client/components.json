{
  "$schema": "https://ui.shadcn.com/schema.json",
  "aliases": {
    "components": "src/components",
    "hooks": "src/hooks",
    "lib": "src/lib",
    "ui": "src/components/plate-ui",
    "utils": "src/lib/utils"
  },
  "iconLibrary": "radix",
  "rsc": false,
  "style": "new-york",
  "tailwind": {
    "baseColor": "neutral",
    "config": "tailwind.config.js",
    "css": "src/index.css",
    "cssVariables": true,
    "prefix": ""
  },
  "tsx": true,
  "registries": {
    "plate": {
      "style": "default",
      "aliases": {
        "ui": "src/components/plate-ui"
      },
      "url": "https://platejs.org/r"
    }
  }
}